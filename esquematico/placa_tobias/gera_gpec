PROJETO="placa_TC"

cd ./gerb
mkdir -p simples

cp $PROJETO".GTL" ./simples/$PROJETO".GTL"
cp $PROJETO".GBL" ./simples/$PROJETO".GBL"
cp $PROJETO".TXT" ./simples/$PROJETO".TXT"
cp $PROJETO".GML" ./simples/$PROJETO".GML"
